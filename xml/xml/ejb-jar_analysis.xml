<?xml version="1.0" encoding="MS932"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 1.1//EN" "http://java.sun.com/j2ee/dtds/ejb-jar_1_1.dtd">

<ejb-jar>
    <display-name>analysisEJB</display-name>
    <enterprise-beans>
        <session>
            <display-name>PEF_MoralSurveyReportEJB</display-name>
            <ejb-name>PEF_MoralSurveyReportEJB</ejb-name>
            <home>jp.co.hisas.career.analysis.report.ejb.PEF_MoralSurveyReportEJBHome</home>
            <remote>jp.co.hisas.career.analysis.report.ejb.PEF_MoralSurveyReportEJB</remote>
            <ejb-class>jp.co.hisas.career.analysis.report.ejb.PEF_MoralSurveyReportEJBBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <resource-ref>
                <res-ref-name>jdbc/HCDB</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
            </resource-ref>
        </session>
        <session>
            <display-name>PEF_JinzaiPortfolioEJB</display-name>
            <ejb-name>PEF_JinzaiPortfolioEJB</ejb-name>
            <home>jp.co.hisas.career.analysis.report.ejb.PEF_JinzaiPortfolioEJBHome</home>
            <remote>jp.co.hisas.career.analysis.report.ejb.PEF_JinzaiPortfolioEJB</remote>
            <ejb-class>jp.co.hisas.career.analysis.report.ejb.PEF_JinzaiPortfolioEJBBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <resource-ref>
                <res-ref-name>jdbc/HCDB</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
            </resource-ref>
        </session>
     </enterprise-beans>
    
    <assembly-descriptor>
        <container-transaction>
    		<method>
    			<ejb-name>PEF_MoralSurveyReportEJB</ejb-name>
    		<method-name>*</method-name>
    		</method>
    		<trans-attribute>Required</trans-attribute>
    	</container-transaction>
        <container-transaction>
    		<method>
    			<ejb-name>PEF_JinzaiPortfolioEJB</ejb-name>
    		<method-name>*</method-name>
    		</method>
    		<trans-attribute>Required</trans-attribute>
    	</container-transaction>

    </assembly-descriptor>
</ejb-jar>
